// Test working builtin functions

print("Testing working builtin functions...");

// Test len function
var message = "Hello World";
print("Length of '" + message + "': " + len(message));

var numbers = [1, 2, 3, 4, 5];
print("Length of numbers: " + len(numbers));

// Test typeOf function
print("Type of message: " + typeOf(message));
print("Type of numbers: " + typeOf(numbers));
print("Type of 42: " + typeOf(42));
print("Type of 3.14: " + typeOf(3.14));

// Test time function
print("Current timestamp: " + time());

// Test date and now functions
print("Current date: " + date());
print("Current date/time: " + now());

// Test round function
var pi = 3.14159;
print("Pi rounded to 2 decimals: " + round(pi, 2));

// Test debug function
print("Debug output for pi:");
debug(pi);

