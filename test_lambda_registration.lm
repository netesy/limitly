// Test lambda function registration timing fix
print("Testing lambda expressions...");

// Test 1: Simple lambda expression
var double_func = fn(x: int): int { return x * 2; };
print("Created lambda function");

// Test 2: Call the lambda function
var result = double_func(5);
print("Lambda result: " + result);

// Test 3: Lambda with variable capture (closure)
var multiplier = 3;
var triple_func = fn(x: int): int { return x * multiplier; };
var triple_result = triple_func(4);
print("Closure result: " + triple_result);

print("Lambda tests completed!");