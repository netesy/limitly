// Simple test for utility functions

print("=== Simple Utility Functions Test ===");

// Create dummy functions for testing
fn add(x: int, y: int) : int {
    return x + y;
}

fn multiply(x: int) : int {
    return x * 2;
}

print("Created test functions: add and multiply");

// Test compose function
print("\n=== Testing compose function ===");
var composed = compose(multiply, add);
print("Composed function created: " + typeOf(composed));

// Test curry function
print("\n=== Testing curry function ===");
var curried = curry(add);
print("Curried function created: " + typeOf(curried));

// Test partial function
print("\n=== Testing partial function ===");
var partial5 = partial(add, 5);
print("Partial function created: " + typeOf(partial5));

print("\n=== Utility function tests completed! ===");