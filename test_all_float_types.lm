// Test f32 precision
var f32_val = 3.14159
print("f32 value: " + f32_val)

// Test f64 precision  
var f64_val = 3.141592653589793
print("f64 value: " + f64_val)

// Test f128 precision
var f128_val = 3.14159265358979323846264338327950288419716939937510
print("f128 value: " + f128_val)

// Test mixed operations
var mixed1 = f32_val + f64_val
print("f32 + f64: " + mixed1)

var mixed2 = f64_val + f128_val
print("f64 + f128: " + mixed2)

var mixed3 = f32_val + f128_val
print("f32 + f128: " + mixed3)

// Test integer-float operations
var int_val = 42
var int_f32 = int_val + f32_val
var int_f64 = int_val + f64_val
var int_f128 = int_val + f128_val

print("int + f32: " + int_f32)
print("int + f64: " + int_f64)
print("int + f128: " + int_f128)

// Test large numbers
var huge_f32 = 1.2345678e+20
var huge_f64 = 1.234567890123456789e+50
var huge_f128 = 1.234567890123456789012345678901234567890123456789e+100

print("huge f32: " + huge_f32)
print("huge f64: " + huge_f64)
print("huge f128: " + huge_f128)
