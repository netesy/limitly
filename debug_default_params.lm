// Test function with default parameter
fn greet(name: str = "World"): str {
    print("Hello, {name}!");
    return "greeted {name}";
}

// Test calling with and without parameter
print("=== Testing Default Parameters ===");
var result1 = greet("Alice");
print("Result 1: {result1}");

var result2 = greet();
print("Result 2: {result2}");