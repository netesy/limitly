// Test print statements - Line by line debugging
print("=== Print Statement Tests ===");

// Basic print
print("Hello, World!");

// Print variables
var name = "Limit";
var version = 1.0;
print(name);
print(version);

// Print expressions
print(2 + 3);
print(10 > 5);
print("Result: " + "Success");

// Print with interpolation
var x = 42;
print("The answer is {x}");

// Print multiple types
print(true);
print(false);
print(nil);
print(0);
print("");

// Print complex expressions
var a = 5;
var b = 3;
print("Math: {a} + {b} = {a + b}");
print("Logic: {a} > {b} is {a > b}");

// Print with special characters
print("Line 1\nLine 2");
print("Tab\tSeparated");
print("Quote: \"Hello\"");
