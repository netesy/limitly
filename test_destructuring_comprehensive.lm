// Comprehensive tuple destructuring test

print("=== Tuple Destructuring Tests ===");

// Test 1: Basic destructuring
print("\n1. Basic destructuring:");
var (a, b, c) = (10, 20, 30);
print("a =", a, "b =", b, "c =", c);

// Test 2: Mixed types
print("\n2. Mixed type destructuring:");
var (num, text, flag) = (42, "hello", true);
print("num =", num, "text =", text, "flag =", flag);

// Test 3: Destructuring from variable
print("\n3. Destructuring from variable:");
var source = (100, 200, 300);
var (x, y, z) = source;
print("source =", source);
print("x =", x, "y =", y, "z =", z);

// Test 4: Destructuring function return
print("\n4. Destructuring function return:");
fn getCoords(): (int, int) {
    return (50, 75);
}

var (coordX, coordY) = getCoords();
print("coordX =", coordX, "coordY =", coordY);

// Test 5: Nested tuple (partial destructuring)
print("\n5. Nested tuple access:");
var nested = ((1, 2), (3, 4));
var (first, second) = nested;
print("first =", first, "second =", second);

// Test 6: Two-element destructuring
print("\n6. Two-element destructuring:");
var (alpha, beta) = (999, 888);
print("alpha =", alpha, "beta =", beta);

print("\n=== All tests completed ===");