// Test cross-module visibility - should be caught at compile-time

import simple_person_module show Person;

fn testCrossModuleAccess() {
    var person = Person("Alice", 25, "123-45-6789");
    
    // Public access - should work
    print("Public name: " + person.name);
    
    // Protected access - should be compile-time error (cross-module)
    print("Protected age: " + person.age);
    
    // Private access - should be compile-time error (cross-module)
    print("Private ssn: " + person.ssn);
}

testCrossModuleAccess();