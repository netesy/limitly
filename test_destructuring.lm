// Test tuple destructuring
print("Testing tuple destructuring...");

// Create a tuple
var source = (10, 20, 30);
print("Source tuple:", source);

// Try destructuring
var (x, y, z) = source;
print("x:", x);
print("y:", y);  
print("z:", z);

// Test with direct tuple
var (a, b) = (100, 200);
print("a:", a);
print("b:", b);