// Test simple higher-order function support without nested functions

// Define simple functions first
fn add_one(x: int): int {
    return x + 1;
}

fn multiply_by_two(x: int): int {
    return x * 2;
}

// Define a function that takes a function parameter
fn apply_function(f: function(int): int, value: int): int {
    return f(value);
}

// Test calling a function with a function parameter
print("Testing function parameters:");
var result1: int = apply_function(add_one, 5);
print("apply_function(add_one, 5) =", result1);

var result2: int = apply_function(multiply_by_two, 3);
print("apply_function(multiply_by_two, 3) =", result2);