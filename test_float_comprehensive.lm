fn main() {
    // Test basic float operations
    var a = 3.14
    var b = 2.0
    var c = a + b
    print("Addition: " + c)
    
    // Test subtraction
    var d = a - b
    print("Subtraction: " + d)
    
    // Test multiplication
    var e = a * b
    print("Multiplication: " + e)
    
    // Test division
    var f = a / b
    print("Division: " + f)
    
    // Test mixed operations
    var g = 5 + 2.5
    print("Mixed int-float addition: " + g)
    
    // Test large float
    var h = 123456789.987654321
    print("Large float: " + h)
}

main();