// Test 4 outer, 3 inner
print("=== Testing 4 Outer, 3 Inner ===");

var outer_list = [1, 2, 3, 4];
var inner_list = ["a", "b", "c"];

print("Nested iter loops:");
iter (var x in outer_list) {
    print("Outer: {x}");
    iter (var y in inner_list) {
        print("  Inner: {y}");
    }
}

print("Done");