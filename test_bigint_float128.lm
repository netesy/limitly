fn main() {
    // Test basic float operations
    var a = 3.141592653589793
    var b = 2.718281828459045
    var c = a + b
    print("Float addition: " + c)
    
    // Test high precision
    var pi = 3.14159265358979323846264338327950288419716939937510
    print("High precision pi: " + pi)
    
    // Test very large numbers
    var huge = 1.23456789012345678901234567890e+100
    print("Huge number: " + huge)
    
    // Test mixed operations
    var int_val = 42
    var float_val = 3.14
    var mixed = int_val + float_val
    print("Mixed int-float: " + mixed)
}

main();
