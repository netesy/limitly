// Fixed comprehensive test for builtin functions
print("=== Testing Builtin Functions ===");

// Test basic math functions
print("--- Math Functions ---");
var negNum: int = -5;
print("abs(-5):");
print(abs(negNum));

print("sqrt(16):");
print(sqrt(16.0));

print("pow(2, 3):");
print(pow(2.0, 3.0));

print("sin(0):");
print(sin(0.0));

print("cos(0):");
print(cos(0.0));

print("pi constant:");
print(pi());

print("e constant:");
print(e());

// Test string functions
print("--- String Functions ---");
var str1: str = "Hello";
var str2: str = " World";
print("concat('Hello', ' World'):");
print(concat(str1, str2));

print("length('Hello'):");
print(length(str1));

print("substring('Hello', 1, 3):");
print(substring(str1, 1, 3));

// Test time functions
print("--- Time Functions ---");
print("time():");
print(time());

print("date():");
print(date());

print("now():");
print(now());

// Test rounding functions
print("--- Rounding Functions ---");
print("ceil(3.2):");
print(ceil(3.2));

print("floor(3.8):");
print(floor(3.8));

print("=== All builtin function tests completed ===");