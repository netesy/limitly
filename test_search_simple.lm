// Simple test for search functions

print("=== Simple Search Functions Test ===");

// Test data
var numbers = [1, 2, 3, 4, 5, 6];
var odds = [1, 3, 5, 7];
var positives = [1, 2, 3];
var withNegative = [-1, 2, 3];

print("Test data:");
print("numbers: " + numbers);
print("odds: " + odds);
print("positives: " + positives);
print("withNegative: " + withNegative);

// Test find function (finds first even number)
print("\n=== Testing find function ===");
var foundEven = find(nil, numbers);
print("First even number in numbers: " + foundEven);

var foundEvenInOdds = find(nil, odds);
print("First even number in odds: " + foundEvenInOdds);

// Test some function (checks if any element is even)
print("\n=== Testing some function ===");
var hasEven = some(nil, numbers);
print("Numbers has even elements: " + hasEven);

var oddsHasEven = some(nil, odds);
print("Odds has even elements: " + oddsHasEven);

// Test every function (checks if all elements are positive)
print("\n=== Testing every function ===");
var allPositive = every(nil, positives);
print("All elements in positives are positive: " + allPositive);

var allPositiveWithNeg = every(nil, withNegative);
print("All elements in withNegative are positive: " + allPositiveWithNeg);

print("\n=== Search function tests completed! ===");