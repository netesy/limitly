// Test unhandled fallible expression detection

fn divide(a: int, b: int): int?DivisionByZero {
    if (b == 0) {
        return err(DivisionByZero);
    }
    return ok(a / b);
}

fn test_unhandled() {
    // This should trigger a compile-time error for unhandled fallible expression
    var result = divide(10, 2);
    print("Result:", result);
    
    // This should be OK because it uses ?
    var handled_result = divide(10, 2)?;
    print("Handled result:", handled_result);
}

test_unhandled();