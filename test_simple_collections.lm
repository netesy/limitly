// Test simple collection builtin functions (without lambdas for now)

// Test data
var numbers = [1, 2, 3, 4, 5];
print("Original numbers: ");
print(numbers);

// Test map function (doubles numbers)
var doubled = map(nil, numbers);  // Using nil as placeholder function
print("Doubled numbers: ");
print(doubled);

// Test filter function (keeps even numbers)
var evens = filter(nil, numbers);  // Using nil as placeholder function
print("Even numbers: ");
print(evens);

// Test reduce function (sums numbers)
var sum = reduce(nil, numbers, 0);  // Using nil as placeholder function
print("Sum of numbers: ");
print(sum);

// Test forEach function (prints each element)
print("forEach output:");
forEach(nil, numbers);  // Using nil as placeholder function

print("Simple collection function tests completed!");