var a = 10;
var b = 3;
print("a = {a}");
print("b = {b}");

// Test simple operations first
var sum = a + b;
print("a + b = {sum}");

var product = sum * 2;
print("(a + b) * 2 = {product}");

var division = a / b;
print("a / b = {division}");

// Now test the complex expression
var result = (a + b) * 2 - a / b;
print("Complex: (a + b) * 2 - a / b = {result}");