// Test to debug type resolution in member access

class TestClass {
    pub publicField: str;
    
    fn init(value: str) {
        this.publicField = value;
    }
}

fn testTypeResolution() {
    var obj = TestClass("test");
    
    // This should work and help us see if type resolution is working
    print("Accessing public field: " + obj.publicField);
}

testTypeResolution();