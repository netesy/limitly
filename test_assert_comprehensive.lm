// Comprehensive test for assert function

print("=== Comprehensive Assert Test ===");

// Test 1: Assert with true condition (should pass)
print("Test 1: Assert with true condition");
assert(true, "This assertion should pass");
print("✅ Assert with true passed");

// Test 2: Assert with false condition (should fail and stop execution)
print("Test 2: Assert with false condition");
print("About to call assert(false, ...)");
assert(false, "This assertion should fail and stop execution");
print("❌ This line should NOT be printed if assert works correctly");

print("Test completed");