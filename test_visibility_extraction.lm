// Test visibility extraction functionality

// Module-level declarations with different visibility levels
pub var publicVar: int = 42;
var privateVar: str = "hidden";
pub fn publicFunction(): int {
    return 10;
}

fn privateFunction(): str {
    return "secret";
}

// Class with mixed visibility members
class TestClass {
    pub var publicField: int;
    var privateField: str;
    
    pub fn publicMethod(): int {
        return this.publicField;
    }
    
    fn privateMethod(): str {
        return this.privateField;
    }
}

// Test inheritance
class ChildClass : TestClass {
    prot var protectedField: bool;
    
    prot fn protectedMethod(): bool {
        return this.protectedField;
    }
}