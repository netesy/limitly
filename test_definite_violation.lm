// Test that should definitely trigger a visibility violation

class TestClass {
    pub publicField: str;
    prot protectedField: int;
    var privateField: bool;  // private by default
    
    pub fn init(pub: str, prot: int, priv: bool) {
        this.publicField = pub;
        this.protectedField = prot;
        this.privateField = priv;
    }
}

fn testViolations() {
    var obj = TestClass("public", 42, true);
    
    // This should work
    print("Public: " + obj.publicField);
    
    // These should be caught by type_checker (but currently aren't)
    print("Protected: " + obj.protectedField);  // Should work in same module
    print("Private: " + obj.privateField);     // Should work in same module
    
    // Try accessing non-existent field - should be caught
    print("NonExistent: " + obj.nonExistentField);
}

testViolations();