// Test with explicit visibility modifiers

// These should be extracted as private (default)
fn implicitPrivateFunc(): str {
    return "implicit private";
}

var implicitPrivateVar: str = "implicit private";

// These should be extracted as public
pub fn explicitPublicFunc(): str {
    return "explicit public";
}

pub var explicitPublicVar: str = "explicit public";

// Test class with explicit visibility
class TestClass {
    // These should be private by default
    var implicitPrivateField: str;
    fn implicitPrivateMethod(): str {
        return "private method";
    }
    
    // These should be public
    pub var explicitPublicField: str;
    pub fn explicitPublicMethod(): str {
        return "public method";
    }
}

fn main() {
    // All of these should work - same module access
    var result1 = implicitPrivateFunc();
    var result2 = explicitPublicFunc();
    var result3 = implicitPrivateVar;
    var result4 = explicitPublicVar;
    
    var obj = TestClass();
    var result5 = obj.implicitPrivateField;
    var result6 = obj.explicitPublicField;
    var result7 = obj.implicitPrivateMethod();
    var result8 = obj.explicitPublicMethod();
}

main();