// Simple test to understand function reference issue

fn addOne(x: int): int {
    return x + 1;
}

// Test 1: Direct function call (should work)
print("Direct call: addOne(5) = " + addOne(5));

// Test 2: Function as variable (should work)
var funcVar: function = addOne;
print("Function variable: " + funcVar);

// Test 3: Simple higher-order function
fn callWithFive(f: function): int {
    print("Received function: " + f);
    return f(5);  // This is where it should fail
}

// Test 4: Call higher-order function with regular function name
print("About to call callWithFive(addOne)...");
var result = callWithFive(addOne);
print("Result: " + result);