// Test basic while loop
var i = 0;
while (i < 5) {
    print( i);
    i = i + 1;
}

print( "Done with while loop");

// Test for loop
for (var j = 0; j < 3; j = j + 1) {
    print( j);
}

print( "Done with for loop");

// Test break and continue
var k = 0;
while (k < 10) {
    if (k == 5) {
        break;
    }
    if (k == 2) {
        k = k + 1;
        continue;
    }
    print( k);
    k = k + 1;
}

print( "Done with break/continue test");

// Test iter loop (basic version)
iter( x in [1, 2, 3] ){
    print( x);
}

print( "Done with iter loop");
