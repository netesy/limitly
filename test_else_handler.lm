// Test ?else syntax

fn divide(a: int, b: int): int?DivisionByZero {
    if (b == 0) {
        return err(DivisionByZero);
    }
    return ok(a / b);
}

fn test_else_handler() {
    // Test successful case
    var result1 = divide(10, 2)? else error {
        print("Error occurred:", error);
        return nil;
    };
    print("Success result:", result1);
    
    // Test error case
    var result2 = divide(10, 0)? else error {
        print("Caught error:", error);
        return nil;
    };
    print("Error result:", result2);
    return nil;
}

test_else_handler();