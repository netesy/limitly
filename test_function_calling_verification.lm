// Test to verify if function calling through variables actually works

fn addOne(x: int): int {
    return x + 1;
}

fn multiplyByTwo(x: int): int {
    return x * 2;
}

// Test higher-order function that actually calls the function parameter
fn processValue(input: int, processor: function): int {
    // Try to call the processor function - let's see if this actually works
    return processor(input);
}

// Test the function calling
print("Testing function calling through variables...");

var result1 = processValue(5, addOne);      // Should return 6 if calling works
var result2 = processValue(10, multiplyByTwo); // Should return 20 if calling works

print("processValue(5, addOne) = " + result1);
print("processValue(10, multiplyByTwo) = " + result2);

if (result1 == 6 and result2 == 20) {
    print("SUCCESS: Function calling through variables works!");
} else {
    print("LIMITATION: Function calling through variables returns default values");
    print("Expected: result1=6, result2=20");
    print("Actual: result1=" + result1 + ", result2=" + result2);
}