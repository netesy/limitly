// Test ?else syntax with success case

fn divide(a: int, b: int): int?DivisionByZero {
    if (b == 0) {
        return err(DivisionByZero);
    }
    return ok(a / b);
}

fn test_success() {
    // Test success case - should not execute else handler
    var result = divide(10, 2)? else error {
        print("This should not print");
        -1
    };
    print("Success result:", result);
    
    // Test error case - should execute else handler
    var error_result = divide(10, 0)? else error {
        print("Caught error:", error);
        99
    };
    print("Error result:", error_result);
}

test_success();