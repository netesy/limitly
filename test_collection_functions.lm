// Test core collection builtin functions

// Test data
var numbers = [1, 2, 3, 4, 5];
print("Original numbers: " + numbers);

// Test map function with a simple lambda
var doubled = map(fn(x) { return x * 2; }, numbers);
print("Doubled numbers: " + doubled);

// Test filter function
var evens = filter(fn(x) { return x % 2 == 0; }, numbers);
print("Even numbers: " + evens);

// Test reduce function
var sum = reduce(fn(acc, x) { return acc + x; }, numbers, 0);
print("Sum of numbers: " + sum);

// Test forEach function
print("Printing each number:");
forEach(fn(x) { print("Number: " + x); }, numbers);

print("Collection function tests completed!");