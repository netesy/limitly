// Simple error handling test
print("Testing simple error handling");

// Test basic division by zero
var result = 10 / 0;
print("Division result:", result);

// Test function with error union return type
fn safe_divide(a: int, b: int): int?DivisionByZero {
    if (b == 0) {
        return err(DivisionByZero);
    }
    return ok(a / b);
}

// Test calling the function
var safe_result = safe_divide(10, 2);
print("Safe division result:", safe_result);

var error_result = safe_divide(10, 0);
print("Error division result:", error_result);