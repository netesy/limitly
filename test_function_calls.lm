fn simple() {
    print("hello from simple");
}

fn withParam(name: str) {
    print("hello {name}");
}

fn withReturn(): str {
    return "returned value";
}

fn withMath(a: int, b: int): int {
    return a + b * 2;
}

// Call the functions
print("Testing function calls:");
simple();
withParam("world");
var result: str = withReturn();
print("Got result: {result}");
var math_result: int = withMath(5, 3);
print("Math result: {math_result}");