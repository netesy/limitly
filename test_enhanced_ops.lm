// Test enhanced unary and binary operations

print("=== Enhanced Operations Test ===");

var a = 10;
var b = 3;

// Unary operations
print("Unary minus: {-a}");
print("Unary plus: {+a}");
print("Logical NOT: {!true}");
print("Bitwise NOT: {~0}");

// Binary operations  
print("Addition: {a + b}");
print("Subtraction: {a - b}");
print("Multiplication: {a * b}");
print("Division: {a / b}");
print("Modulo: {a % b}");

// Comparisons
print("Equal: {a == b}");
print("Not equal: {a != b}");
print("Less: {a < b}");
print("Less equal: {a <= b}");
print("Greater: {a > b}");
print("Greater equal: {a >= b}");

// Bitwise operations
print("Bitwise AND: {a and b}");
print("Bitwise OR: {a or b}");
// print("Bitwise XOR: {a ^ b}");

// Power operation (simplified)
print("Power: {a ** 2}");
