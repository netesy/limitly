// First test: simple dict creation and access
var person: dict = {name: "John", age: 30};
print("Created person dict");

// Test basic match with wildcard
match(person) {
    _ => {
        print("Wildcard match works");
    }
}

// Test simple variable binding
match(person) {
    p => {
        print("Variable binding works");
    }
}

