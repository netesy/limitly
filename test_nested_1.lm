// Test nested loop with 1 inner element
print("=== Testing Nested Loop with 1 Inner Element ===");

var outer_list = [1, 2, 3];
var inner_list = ["a"];

print("Nested iter loops:");
iter (var x in [1, 2, 3,4,5,6]) {
    print("Outer: {x}");
    iter (var y in inner_list) {
        print("Outer: {x}  Inner: {y}");
    }
}

iter (var x in 1..3) {
    print("Outer {x} has error when we have nested ranges with the variable scoping");
    iter (var j in 10..12) {
        print("Inner: {j}")
        print("({x}, {j})");
    }
}

iter (var x in [1, 2, 3]) {
    print("Outer {x} works because we use list");
    iter (var j in 10..12) {
        print("({x}, {j})");
    }
}

iter (var i in [1, 2, 3, 4, 5, 6, 7, 8]) {
    if( i == 7) {
        break;
    }
    print("Before inner: {i}");
    
    iter (var j in [10, 20, 30]) {
        if( j == 20) {

            continue;
        }
        print("  Inner: {j}");
    }
}
print("Done");