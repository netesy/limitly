print("=== Basic Guard Tests ===");

// Test simple guards without function calls
var value = 5;
match(value) {
    x where x > 10 => { print("{x} is greater than 10"); },
    x where x < 10 => { print("{x} is less than 10"); },
    x => { print("{x} is exactly 10"); }
}

