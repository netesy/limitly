var x:int = 5;

match (x) {
    5 => {
        print("x is five");
    },
    10 => {
        print("x is ten");
    },
    _ => {
        print("x is something else");
    }
}

var value = false;
match (value) {
        int => {
             print("Integer: {value}");
             },
        str => {
             print("String: {value}");
             },
        list => {
             print("List of integers: {value}"); },
        dict => { print("Dictionary: {value}"); },
        _ => { print("Unknown type");
    }
}


    match(18) {
        x where x > 10 => { print("{x} is greater than 10"); },
        x where x < 10 => { print("{x} is less than 10"); },
        x => { print("{x} is 10"); }
    }

