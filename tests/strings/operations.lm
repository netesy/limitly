// Test string operations
print("=== String Operations Tests ===");

var str1 = "Hello";
var str2 = "World";
var space = " ";

// String concatenation
assert(str1 + space + str2 == "Hello World", "String concatenation failed");

// String comparison
print("String comparisons:");
assert(!(str1 == str2), "str1 == str2 should be false");
assert(str1 != str2, "str1 != str2 should be true");
assert(str1 == "Hello", "str1 == 'Hello' should be true");

// String with numbers
var numStr = "42";
var num = 42;
assert(numStr == "42", "String vs number comparison failed");

// Empty strings
var empty = "";
assert(empty == "", "Empty string check failed");
var emptyStr = "";
assert(empty == emptyStr, "Empty string comparison failed");

// Special characters
var special = "Line 1\nLine 2\tTabbed";
assert(special == "Line 1\nLine 2\tTabbed", "Special characters in string failed");

// Quotes in strings
var quoted = "He said \"Hello\"";
assert(quoted == "He said \"Hello\"", "Quotes in string failed");