// Comprehensive test for built-in error types

print("=== Testing Built-in Error Types ===");

// Test 1: Division by zero error
print("Test 1: Division by zero");
var result1 = 10 / 0;
print("Division result:", result1);

// Test 2: List index out of bounds
print("Test 2: List index out of bounds");
var myList = [1, 2, 3];
var result2 = myList[5];  // Should return IndexOutOfBounds error
print("List access result:", result2);

// Test 3: Dictionary key not found
print("Test 3: Dictionary key not found");
var myDict = {"a": 1, "b": 2};
var result3 = myDict["c"];  // Should return IndexOutOfBounds error
print("Dict access result:", result3);

// Test 4: Invalid list index type
print("Test 4: Invalid list index type");
var result4 = myList["invalid"];  // Should return TypeConversion error
print("Invalid index result:", result4);

// Test 5: Indexing non-container
print("Test 5: Indexing non-container");
var number = 42;
var result5 = number[0];  // Should return TypeConversion error
print("Non-container index result:", result5);

print("=== Built-in Error Types Test Complete ===");