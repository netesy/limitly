// Basic union type parsing tests

// Simple union types with user-defined types
type Option = Some | None;

// Union with primitive types
type NumberType = int | f64;

// Union with mixed primitive and user-defined types
type StringOrNumber = str | NumberType;

// Multi-variant union types
type Color = Red | Green | Blue | Yellow;
type Status = Success | Warning | Error | Info;

// Nested union types
type Result = Success | Error;
type MaybeResult = Result | None;

// Union with primitive and user-defined types
type Value = int | str | bool | User;

// Print test message
print("Basic union type parsing test completed");