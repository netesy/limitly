// Union type parsing tests

// Basic union types
type Option = Some | None;
type NumberType = int | f64;
type StringOrNumber = str | NumberType;

// Complex union types with structured variants
type Some = { kind: "Some", value: any };
type None = { kind: "None" };

// Multi-variant union types
type Color = Red | Green | Blue | Yellow;
type Status = Success | Warning | Error | Info;

// Nested union types
type Result = Success | Error;
type MaybeResult = Result | None;

// Union with primitive and user-defined types
type Value = int | str | bool | User;

// Union with list types
type ListOrValue = [int] | int;

// Union with structural types
type Response = { status: "ok", data: any } | { status: "error", message: str };

// Print test message
print("Union type parsing test completed");