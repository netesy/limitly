// Comprehensive typed container tests for task 16

// Test 1: Basic typed list declarations with type checker validation
type IntList = [int];
type StringList = [str];
type FloatList = [float];
type BoolList = [bool];

// Test 2: Basic typed dictionary declarations with type checker validation
type IntToStringDict = {int: str};
type StringToIntDict = {str: int};
type StringToFloatDict = {str: float};
type IntToFloatDict = {int: float};

// Test 3: Nested typed containers
type ListOfIntLists = [[int]];
type DictOfStringToIntList = {str: [int]};
type ListOfStringToIntDict = [{str: int}];

// Test 4: Variable declarations with typed containers - type checker validation
var numbers: [int] = [1, 2, 3, 4, 5];
var words: [str] = ["hello", "world", "test"];
var flags: [bool] = [true, false, true];
var scores: [float] = [95.5, 87.2, 92.8];

print("=== Typed Lists ===");
print("numbers:", numbers);
print("words:", words);
print("flags:", flags);
print("scores:", scores);

// Test 5: Dictionary variables with typed containers - type checker validation
var ageMap: {str: int} = {"Alice": 25, "Bob": 30, "Charlie": 35};
var priceMap: {int: float} = {1: 9.99, 2: 19.99, 3: 29.99};
var nameMap: {int: str} = {1: "First", 2: "Second", 3: "Third"};

print("\n=== Typed Dictionaries ===");
print("ageMap:", ageMap);
print("priceMap:", priceMap);
print("nameMap:", nameMap);

// Test 6: Complex nested containers with type checker validation
var matrix: [[int]] = [[1, 2], [3, 4], [5, 6]];
var studentGrades: {str: [float]} = {
    "Alice": [95.5, 87.2, 92.8],
    "Bob": [88.1, 91.5, 89.7]
};

print("\n=== Nested Containers ===");
print("matrix:", matrix);
print("studentGrades:", studentGrades);

// Test 7: Container type inference from initialization values
var inferredNumbers = [1, 2, 3];  // Should infer [int]
var inferredWords = ["a", "b"];   // Should infer [str]
var inferredAges = {"Alice": 25}; // Should infer {str: int}

print("\n=== Type Inference ===");
print("inferredNumbers:", inferredNumbers);
print("inferredWords:", inferredWords);
print("inferredAges:", inferredAges);

print("\nAll typed container tests passed!");