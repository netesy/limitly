// Test arithmetic expressions
print("=== Arithmetic Expression Tests ===");

var a = 10;
var b = 3;

// Basic arithmetic
assert(a + b == 13, "Addition failed");
assert(a - b == 7, "Subtraction failed");
assert(a * b == 30, "Multiplication failed");
assert(a / b == 3, "Integer division failed");
assert(a % b == 1, "Modulo failed");

// Unary operations
assert(-a == -10, "Negation failed");
assert(+a == 10, "Positive failed");

// Complex expressions
var result = (a + b) * 2 - a / b;
assert(result == 23, "Complex expression failed");

// Operator precedence
var precedence = a + b * 2;
assert(precedence == 16, "Operator precedence failed");

// Float arithmetic
var x = 3.14;
var y = 2.0;
assert(x + y == 5.14, "Float addition failed");
assert(x / y == 1.57, "Float division failed");