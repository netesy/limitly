// Performance benchmark test file
// This file contains moderately complex code to test CST parser performance

// Test multiple variable declarations
var a = 1;
var b = 2;
var c = 3;
var d = 4;
var e = 5;

// Test multiple expressions
var sum1 = a + b + c;
var sum2 = d + e;
var product = sum1 * sum2;
var difference = product - sum1;

// Test nested control structures
for (var outer = 0; outer < 5; outer = outer + 1) {
    for (var inner = 0; inner < 3; inner = inner + 1) {
        if (outer == inner) {
            print("Equal: " + str(outer) + ", " + str(inner));
        } else if (outer > inner) {
            print("Greater: " + str(outer) + ", " + str(inner));
        } else {
            print("Less: " + str(outer) + ", " + str(inner));
        }
    }
}

// Test multiple print statements
print("Sum1: " + str(sum1));
print("Sum2: " + str(sum2));
print("Product: " + str(product));
print("Difference: " + str(difference));

// Test more complex expressions
var complex1 = (a + b) * (c + d) - e;
var complex2 = a * b + c * d + e;
var complex3 = (complex1 + complex2) / 2;

print("Complex1: " + str(complex1));
print("Complex2: " + str(complex2));
print("Complex3: " + str(complex3));