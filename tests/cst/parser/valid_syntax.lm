// Test valid syntax parsing in CST parser
// This file contains various valid language constructs to test CST generation

// Variable declarations
var x = 42;
var y = "hello";
var z = true;

// Simple expressions
var sum = x + 10;
var product = x * 2;

// Control flow
if (x > 0) {
    print("positive");
} else {
    print("non-positive");
}

// Simple loop
for (var i = 0; i < 3; i = i + 1) {
    print("Loop: " + str(i));
}

// String operations
print("Result: " + str(sum));
print("Product: " + str(product));