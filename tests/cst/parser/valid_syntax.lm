// Test valid syntax parsing in CST parser
// This file contains various valid language constructs to test CST generation

fn main() {
    // Variable declarations
    var x: int = 42;
    var y: str = "hello";
    var z: bool = true;
    
    // Control flow
    if (x > 0) {
        print("positive");
    } else {
        print("non-positive");
    }
    
    // Loops
    for (var i = 0; i < 5; i = i + 1) {
        print("Loop: {i}");
    }
    
    iter (i in 1..10) {
        if (i % 2 == 0) {
            print("Even: {i}");
        }
    }
    
    // Function calls
    greet("World");
    var result = add(10, 20);
    
    // Expressions
    var complex = (x + y.length()) * 2 - z ? 1 : 0;
    
    // String interpolation
    print("Result: {result}, Complex: {complex}");
}

fn greet(name: str) {
    print("Hello, {name}!");
}

fn add(a: int, b: int) -> int {
    return a + b;
}