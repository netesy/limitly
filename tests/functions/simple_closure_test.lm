print("=== Simple Closure Test ===");

fn simpleAdd(a: int, b: int): int {
    return a + b;
}

var result = simpleAdd(5, 3);
print("Simple function result: " + result);

var simpleLambda = fn(x: int): int {
    return x * 2;
};

var lambdaResult = simpleLambda(5);
print("Lambda result: " + lambdaResult);

print("=== Test Complete ===");