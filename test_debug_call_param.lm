// Debug calling function parameter

// Function that tries to call its parameter
fn test_call(f: function(int): int): int {
    print("About to call f with 10");
    var result = f(10);
    print("f(10) returned:", result);
    return result;
}

// Test with a simple lambda
print("Testing function parameter call:");
var lambda = fn(x: int): int { return x * 3; };
var final_result = test_call(lambda);
print("Final result:", final_result);