// Debug higher-order function step by step

// Simple function that just prints its parameter type
fn test_param(f: function(int): int): int {
    print("Parameter f received");
    return 42;
}

// Test with a simple lambda
print("Testing parameter passing:");
var lambda = fn(x: int): int { return x * 2; };
var result = test_param(lambda);
print("Result:", result);