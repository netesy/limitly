// Simple test to debug function parameter issue

fn addOne(x: int): int {
    return x + 1;
}

fn testFunction(f: function, value: int): int {
    print("Inside testFunction, f =", f);
    print("Inside testFunction, value =", value);
    return f(value);
}

print("Step 1: Direct function call");
var result1 = addOne(5);
print("addOne(5) =", result1);

print("Step 2: Function as parameter");
var result2 = testFunction(addOne, 5);
print("testFunction(addOne, 5) =", result2);