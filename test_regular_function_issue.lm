// Test to demonstrate the issue with regular function names as higher-order parameters

print("=== Regular Function as Parameter Issue Test ===");

// Define a regular function
fn addOne(x: int): int {
    return x + 1;
}

// Define a higher-order function that takes a function parameter
fn processValue(value: int, processor: function): int {
    print("Received value: " + value);
    print("Received processor: " + processor);
    
    // This is where the issue occurs - trying to call a regular function passed as parameter
    return processor(value);  // This should work but currently fails
}

print("\n--- Test 1: Lambda as Parameter (WORKS) ---");
var myLambda = fn(x: int): int {
    return x * 2;
};

print("Testing with lambda...");
var result1 = processValue(5, myLambda);
print("Result with lambda: " + result1);

print("\n--- Test 2: Regular Function as Parameter (FAILS) ---");
print("Testing with regular function name...");
// This is the failing case - passing a regular function name
var result2 = processValue(5, addOne);  // This should work but fails
print("Result with regular function: " + result2);

print("\n=== Test Complete ===");