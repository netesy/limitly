// Simple test of visibility keywords parsing

data class Point {
    x: float;
    y: float;
}

abstract class Shape {
    pub name: str = "shape";
    prot area: float = 0.0;
    const id: int = 1;
    static count: int = 0;
    
    pub fn getName() : str {
        return self.name;
    }
    
    abstract fn calculateArea() : float;
    
    final fn describe() {
        print("Shape description");
    }
    
    static fn getCount() : int {
        return Shape.count;
    }
}

final class Circle : Shape {
    pub radius: float = 1.0;
    
    fn calculateArea() : float {
        return 3.14 * self.radius * self.radius;
    }
}