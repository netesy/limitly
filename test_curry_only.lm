// Test curry function only

print("Starting curry test...");

fn add(x: int, y: int) : int {
    return x + y;
}

print("Function defined");

// Test curry function
print("Calling curry...");
var curried = curry(add);
print("Curry result: " + curried);
print("Curry result type: " + typeOf(curried));

print("Test completed");