for (var i = 0; i < 10; i += 1) {
    print(i);
}
print("2 + 2 = {2 + 2}" );
// Test range iteration
print("Testing range iteration:")
iter (i in 1..5) {
    print(i);
    print("Range iteration: {i}");
}

// Test list iteration
var numbers = [10, 20, 30, 40, 50]
print("\nTesting list iteration:")
iter (num in numbers) {
    print("List element: {num}");
}

// Test nested iteration
print("\nTesting nested iteration:")
iter (i in 1..3) {
    iter (j in 10..13) {
        print("({i}, {j})");
    }
}
